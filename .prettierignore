# Ignore build output
dist/
.vercel/
.build/

# Node modules
node_modules/

# Logs and temporary files
*.log
*.tmp

# Test coverage
coverage/

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore minified files
*.min.js
*.min.css

# Ignore assets
*.png
*.jpg
*.jpeg
*.svg
*.gif
*.webp

# Ignore generated files
astro/types.d.ts

# Ignore Astro files as they cause parsing errors in some environments
**/*.astro
