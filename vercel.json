{
  "version": 2,
  "framework": "astro",
  "buildCommand": "astro build",
  "devCommand": "astro dev",
  "installCommand": "pnpm install",
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/functions/entry.mjs"
    }
  ]
}
