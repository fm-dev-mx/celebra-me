{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"language": "en,es",
	"dictionaryDefinitions": [],
	"dictionaries": [
		"en_US",
		"es",
		"companies",
		"softwareTerms",
		"misc",
		"typescript",
		"css",
		"html"
	],
	"ignorePaths": ["node_modules/**", "dist/**", "build/**", ".git/**", ".cache/**"],
	"allowCompoundWords": true,
	"words": ["celebra-me", "astrojs", "tailwindcss", "supabase", "vercel"],
	"enabledLanguageIds": ["javascript", "typescript", "json", "markdown", "astro"],
	"languageSettings": [
		{
			"languageId": "markdown",
			"allowCompoundWords": true
		}
	]
}
